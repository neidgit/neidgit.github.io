<script type="text/javascript">
$(function(){
	//左侧导航栏 咨询助手效果
	var len=$('.qie_roll').width();
	var wall=0;
	/*气泡提示效果===start*/
	$(".kjmenu ul li").hover(function(){
		$(".qptx",this).show();
	});
	$(".kjmenu ul li").mouseleave(function(){
		$(".qptx",this).hide();
	});
	/*气泡提示效果===end*/
	/*左侧导航鼠标移入移出的效果==start*/
	$('.qie_text').each(function(){
		var b=$(this).find('.qie_roll').length;
		wall=len*b;	
		$(this).width(wall);
	})
	$('.qie_right').click(function(){
		var sz=$(this).parents('.remind').find('.qie_roll').length;
		var wh=len*(sz-2);
		var wid=len*(sz-1);
		$(this).parents('.remind').find('.qie_left').removeClass('qie_left1');
		var l=$(this).parents('.remind').find('.qie_text').css('left');
		if(Math.abs(parseInt(l))>=wh){
			$(this).addClass('qie_right1');
			$(this).parents('.remind').find('.qie_text').animate({'left':'-'+wid+'px'})
		}else{
			$(this).parents('.remind').find('.qie_text').animate({'left':'-='+len+'px'})
		}
	})
	$('.qie_left').click(function(){
		$(this).parents('.remind').find('.qie_right').removeClass('qie_right1');
		var l=$(this).parents('.remind').find('.qie_text').css('left');
		var wh=len*(-1);
		if(l>=wh+"px"){
			$(this).addClass('qie_left1');
			$(this).parents('.remind').find('.qie_text').animate({'left':'0px'})
		}else{
			$(this).parents('.remind').find('.qie_text').animate({'left':'+='+len+'px'})
		}
	})
	/*左侧导航鼠标移入移出的效果==end*/
});
/**保存留言、举报信息**/
function saveReport(){
	var title = $("#title").val();
	if(title == "请输入标题(必填)"){
		title = "";
	}
	if(null==title || ""==title){
		alert("标题不可为空",8);
		$("#title").focus();
		return;
	}
	if(len(title)>100){
		alert("标题长度不可超过50个汉字",8);
		$("#title").focus();
		return;
	}
	var content = $("#content").val();
	if(content == "请在此输入留言内容，我们会尽快与您联系。(必填)"){
		content = "";
	}
	if(null==content || ""==content){
		alert("内容不可为空",8);
		$("#content").focus();
		return;
	}
	if(len(content)>1000){
		alert("内容长度不可超过500个汉字",8);
		$("#content").focus();
		return;
	}
	var username = $("#username").val();
	if(username == "姓名"){
		username = "";
	}
	var mobile = $("#mobile").val();
	if(mobile == "电话"){
		mobile = "";
	}
	if(null!=mobile && ''!=mobile){
		if(isNumber(mobile) && cTrim(mobile,2).length==11){
			//var regu =/^\d[1]{9}$/; 
			var regu =/^1[\d]{10}$/;
			if (!regu.test(mobile)) { 
				alert("联系方式格式错误！",8);
				return;
			}
		}else{
			//var regMov=/(^[0-9]{3,4}\-[0-9]{3,9}$)|(^[0-9]{3,4}\-[0-9]{3,4}\-[0-9]{3,5}$)|(^[0-9]{3,9}$)|(^\([0-9]{3,4}\)[0-9]{3,9}$)|(^0{0,1}13[0-9]{9}$)/;  //电话号码 //电话号码   /^([0-9]{3}-?[0-9]{8})|([0-9]{4}-?[0-9]{8}$)/
			var regMov=/\d{3}-\d{8}|\d{4}-\d{7}/;
  			if(!regMov.test(mobile)){
				alert("联系方式格式错误！",8);
				return;
			}
		}
	}
	$.ajax({
	    url:"/www.nm12366.net:80/foot/saveReport",
	    async: false,
	    cache: false, 
	    type:'POST',
	    data:{"title":title,"content":content,"username":username,"mobile":mobile},
	      	dataType: "xml",
	      	success: function(msg) {
			var code=$(msg).find("code").text();
			var message=$(msg).find("message").text();
			if("0000"==code){
				layer.alert("发送成功，若您留下了联系方式，管理员会在处理之后给予您回复。感谢您的支持！",1,function(){
					window.location.reload();
					});
			}else{
				alert(message);
			}
	       },
	       error:function(data){
			//alert("服务器繁忙，请稍候重试。");
	    }
	});
}

function showLinkLists(showDivId){
	 $.ajax({
			url: '/www.nm12366.net:80/org/sLink',
			data:"opt_flag=qry&if_flag=one&org_id=31500000000&type=1&scope=2&islink=1",
			type:'POST',
			dataType :"xml",
			success:function(msg){
				var code = $(msg).find("code").text();
				var message = $(msg).find("message").text();
				if("0000"==code){//接口调用成功
					var htmlStr = "";
					var htmlStr_left = "";
					var link_l=$(msg).find("Link").length;
					$(msg).find("Link").each(function(i){
						var id=$(this).find("id").text();
						var name=$(this).find("name").text();
						var photo=$(this).find("photo").text();
						var intro=$(this).find("intro").text();
						var linkns=name.split("/");
						if(linkns&&linkns.length==2){
							if(user_type=="2"){
								name=linkns[0];											
							}else{
								name=linkns[1];
							}
						}
						var photo_url="";
						if(null!=photo && ""!=photo){
							if(photo.indexOf("http") > -1){
								if(photo.indexOf("UploadModule")>-1){
									photo_url=photo.substr(photo.indexOf("UploadModule/uploads")+21,photo.length);
									photo_url ="/182.18.63.156:28007/UploadModule/uploads/"+photo_url;
								}else{
									photo_url =photo;
								}
							}else{
								photo_url ="/182.18.63.156:28007/UploadModule/uploads/"+photo;
							}
						}else{
								photo_url="/www.nm12366.net:80/images/nmgs/icon_zx.png";
						}
						
						htmlStr+="<a href=\"javascript:void(0);\" onclick=\"startZx();\">"+name+"</a>";
						if(((i+1)%10)==1){
							htmlStr_left+="<ul class=\"qie_con\"><li><a href=\"javascript:void(0)\" onclick=\"startZx();\">"+name+"</a></li>";
						}else if(((i+1)%10)==0){
							htmlStr_left+="<li><a href=\"javascript:void(0)\" onclick=\"startZx();\">"+name+"</a></li></ul>";
						}else{
							htmlStr_left+="<li><a href=\"javascript:void(0)\" onclick=\"startZx();\">"+name+"</a></li>";
						}
					});
					if(null!=htmlStr && ''!=htmlStr){
						$("#"+showDivId).html(htmlStr);
					}else{
						$("#"+showDivId).html(htmlStr);
					}
					if(null==htmlStr || ''==htmlStr){
						$("#links_left").remove();
					}else{
						if(link_l%10==0){
						}else{
							htmlStr+="</ul>";
						}
						$("#links_left").html(htmlStr_left);
						left_link_qiehuan();
					}
				}else{
					//alert("服务链出现异常："+message);
				}
			}
		});
	}

function left_link_qiehuan(){
	 var len=$('.qie_con').width();
       var wall=0;
       $('.zxzs_con').each(function(){
           var b=$(this).find('.qie_con').length;
           wall=len*b; 
           $(this).width(wall);
       })
       $('.zxzs_qie_right').click(function(){
               var sz=$(this).parents('.zxzs_text').find('.qie_con').length;
               var wh=len*(sz-2);
               var wid=len*(sz-1);
               $(this).parents('.zxzs_text').find('.zxzs_qie_left').removeClass('zxzs_qie_left1');
               var l=$(this).parents('.zxzs_text').find('.zxzs_con').css('left');
               if(Math.abs(parseInt(l))>=wh){
                   $(this).addClass('zxzs_qie_right1');
                   $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'-'+wid+'px'})
               }else{
                   $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'-='+len+'px'})
               }
           })
       $('.zxzs_qie_left').click(function(){
               $(this).parents('.zxzs_text').find('.zxzs_qie_right').removeClass('zxzs_qie_right1');
               var l=$(this).parents('.zxzs_text').find('.zxzs_con').css('left');
               var wh=len*(-1);
               if(l>=wh+"px"){
                   $(this).addClass('zxzs_qie_left1');
                   $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'0px'})
               }else{
                   $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'+='+len+'px'})
               }
           });
	}

function startZx(){
	alert("请先登陆您的账号！");
	return;
}
